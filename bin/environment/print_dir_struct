#!/usr/bin/env bash
set -eu

#######################################
# 参数:
#   $1: REPO_ROOT_DIR
#######################################

clear
echo ''

head -n 66 "$1/docs/结构.md" | tail -n 61 > /tmp/struct.txt

while read -r line; do
  echo "$line"
  sleep 0.1   
done < /tmp/struct.txt

echo ''

read -r -p '输入回车继续...' _
